SELECT *
  FROM (
    SELECT DATE(tgl_trx) trx_date, 
    SUM(trx_count) sum_trx,
    SUM(price) total_trx
    FROM (
      SELECT tgl_trx, COUNT(tgl_trx) trx_count, SUM(total_harga) price
      FROM `gjk-pac-dev-02.dummy_test.penjualan`
      GROUP BY tgl_trx
      ORDER BY tgl_trx
    )
  GROUP BY trx_date
  ORDER BY trx_date
)
WHERE sum_trx > 5;
